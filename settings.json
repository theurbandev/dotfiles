    "backgroundColor": "black !important",
    "frame": false,
    "transparent": true,
    "hasShadow": false,
    "titleBarStyle": "hiddenInset",
    "trafficLightPosition": {
      "x": 7,
      "y": 5
    },
    "visualEffectState": "active"
